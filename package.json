{
  "name": "dlite-web",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "webpack -p --config webpack.config.js",
    "start": "node_modules/knex/bin/cli.js migrate:latest && node index.js",
    "webpack-watch": "webpack -w --config webpack.config.dev.js",
    "dev": "APP_ENV=development concurrently --kill-others \"npm run webpack-watch\" \"nodemon index.js\"",
    "test": "npm run test:client && npm run test:server && npm run test:acceptance",
    "test:server": "node_modules/.bin/mocha --recursive test/server",
    "test:client": "node_modules/.bin/mocha --compilers js:babel-core/register --recursive test/client",
    "test:acceptance": "node_modules/.bin/cucaroo",
    "test:coverage": "npm run test:acceptance && npm run test:client:coverage && npm run test:server:coverage && node_modules/.bin/istanbul report",
    "test:client:coverage": "node_modules/.bin/istanbul cover -dir=coverage/client node_modules/.bin/_mocha -- --require babel-core/register --recursive test/client",
    "test:server:coverage": "node_modules/.bin/istanbul cover -dir=coverage/server node_modules/.bin/_mocha -- --recursive test/server",
    "test:security": "node_modules/.bin/nsp check",
    "lint": "node_modules/.bin/jshint client/ test/ server/ server.js index.js",
    "schema:export": "node bin/export-db-schema.js"
  },
  "author": "Kane Baccigalupi",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-generator": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "compass-mixins": "^0.12.10",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "cssurf": "https://github.com/CDTDigital/cssurf.git",
    "cucaroo": "^0.4.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "jshint": "^2.9.4",
    "knex": "^0.13.0",
    "mocha": "^3.4.2",
    "mock-require": "^2.0.2",
    "node-horseman": "^3.3.0",
    "node-mocks-http": "^1.6.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "pg": "^7.3.0",
    "pg-json-schema-export": "^0.13.7",
    "phantomjs": "^2.1.7",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-test-utils": "^0.2.2",
    "sass-loader": "^6.0.5",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "express-static-gzip": "^0.3.2",
    "helmet": "^3.6.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-oauth2": "^1.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
